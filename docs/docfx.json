{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**/src/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0;netstandard2.1;net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "modern"
    ],
    "namespaceLayout": "nested",
    "globalNamespaceId": "global",
    "markdownEngineName": "markdig",
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "keepFileLink": false,
    "noLangKeyword": false,
    "disableDefaultFilter": true,
    "enumSortOrder": "declaringOrder",
    "memberLayout": "separatePages",
    "globalMetadata": {
      "_appName": "Bodu",
      "_appTitle": "Bodu Documentation",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
