{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [ "**/src/*.csproj" ]
        }
      ],
      "dest": "api",
      "namespaceLayout": "nested",
      "memberLayout": "separatePages",
      "enumSortOrder": "declaringOrder",
      "globalNamespaceId": "global",
      "properties": {
        "TargetFramework": "net8.0"
      },
    },
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "dest": "_site",
    "template": [ "default", "modern" ],
    "markdownEngineName": "markdig",
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "keepFileLink": false,
    "noLangKeyword": false,
    "disableDefaultFilter": true,
    "globalMetadata": {
      "_appName": "BODU",
      "_appTitle": "BODU",
      "_enableSearch": true,
      "pdf": false,
      "_disableContribution": true,
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_appFooter": "Generated by <strong>DocFX</strong> | Documentation Â© 2024 by NAME is licensed under LICENSE"
    }
  }
}